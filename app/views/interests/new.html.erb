<h1>What are your interests?</h1>

<%= form_for(@interest) do |f| %>
  <div id="interest">
    <%= f.fields_for :interests do |ff| %>
      <%= render 'interest_fields', f: ff %>
    <% end %>

    <div class = "links">
      <%= link_to_add_association 'Add Interest', f, :interests %>
    </div>
  </div>
<% end %>
