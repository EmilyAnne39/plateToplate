<div class="nested-fields">
  <%= f.collection_select(:cuisine_id, Cuisine.all, :id, :name) %>
  <%= f.label "I want to learn from others about this cuisine!   "%>
  <%= f.check_box :learn %>
  <%= f.label "I'd be interested to teach others about this cuisine!   "%>
  <%= f.check_box :teach %>
  <%= f.number_field :user_id, value: current_user.id, class: "cant-see-me" %>
  <%= link_to_remove_association "Remove Cuisine", f %>
</div>
